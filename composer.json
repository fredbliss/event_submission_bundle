{
    "name": "intelligentspark/event_submission_bundle",
    "description": "Frontend Event Submission Bundle for Contao 4",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Fred Bliss",
            "email": "fred@intelligentspark.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "contao/core-bundle": "^4.8",
        "contao/calendar-bundle": "^4.8"
    },
    "conflict": {
      "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "extra": {
      "contao-manager-plugin": "IntelligentSpark\\EventSubmission\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "IntelligentSpark\\EventSubmission\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
          "src/Resources/contao/config/",
          "src/Resources/contao/dca/",
          "src/Resources/contao/languages/",
          "src/Resources/contao/templates/"
        ]
    }
}
